{
  "manifest_version": 3,
  "name": "MidoriVPN",
  "short_name": "MidoriVPN",
  "description": "Powerful VPN for Midori Browser & Browsers based Chromium",
  "version": "1.0.3",
  "author": "Astian Inc",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "proxy",
    "storage",
    "webRequest",
    "privacy",
    "alarms",
    "management",
    "scripting",
    "webNavigation",
    "identity",
    "webRequestBlocking",
    "browsingData"
  ],
  "host_permissions": ["https://*.vpn.astian.org/*", "https://*.astian.org/*"]
}
